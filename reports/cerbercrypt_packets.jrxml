	<queryString language="bgbs"><![CDATA[import java.util.*;
import java.sql.*;
import bitel.billing.server.util.*;
import bitel.billing.server.cerbercrypt.bean.*;
import ru.bitel.bgbilling.server.util.*;
import ru.bitel.common.*;


public void fillReport( con, filter, result )
{
	int mid=result.getMid();
	int sumCount=0;
	Calendar date1= filter.getCalendarParam("monthAndDays1");
	String tableName=ServerUtils.getModuleMonthTableName( "packet_detail", date1.getTime(), mid );
	if(!ServerUtils.tableExists(con,tableName))
	{	
		print(tableName+" table not exist...");
		return;
	}
	
	Calendar date2= filter.getCalendarParam("monthAndDays2");
	String day1=String.valueOf(date1.get( Calendar.DAY_OF_MONTH ));
	String day2=String.valueOf(date2.get( Calendar.DAY_OF_MONTH ));
		
	String query = "SELECT sum(summa) as summa, tpacket.title as packet"+
			" FROM "+tableName+" as packet_detail join packet_"+mid+" as tpacket on tpacket.id=packet_detail.pid"+
			" where day >="+day1+" and day<="+day2+" group by pid";

	PreparedStatement ps = con.prepareStatement( query );
	ResultSet rs = ps.executeQuery();
	result.setDataSource( rs );
	
	
	Map params = new HashMap();
	query = "SELECT sum(summa) as summ  FROM "+tableName+" as packet_detail where day >="+day1+" and day<="+day2;
 	ps = con.prepareStatement( query );
	rs = ps.executeQuery();
	if(rs.next())
	{
		params.put( "sum", rs.getBigDecimal(1));
	}	
	
	params.put( "date1", date1.getTime() );
	params.put( "date2", date2.getTime() );
	
	result.setParams( params );
	
		
	
}]]></queryString>

